{
    "redirects": [
      {
        "source": "/style.css",
        "destination": "/style.css",
        "statusCode": 200
      },
      {
        "source": "/main.js",
        "destination": "/main.js",
        "statusCode": 200
      },
      {
        "source": "/assets/:path*",
        "destination": "/assets/:path*",
        "statusCode": 200
      },
      {
        "source": "/components/:path*",
        "destination": "/components/:path*",
        "statusCode": 200
      },
      {
        "source": "/constants.js",
        "destination": "/constants.js",
        "statusCode": 200
      },
      {
        "source": "/(.+)",
        "destination": "https://short-server.onrender.com/shorty/$1",
        "permanent": false
      }
    ]
  }
  